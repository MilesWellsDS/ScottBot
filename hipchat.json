{
  "capabilities": {
    "hipchatApiConsumer": {
      "scopes": [
        "send_notification"
      ]
    },
    "installable": {
      "allowGlobal": false,
      "allowRoom": true
    },
    "webhook": [
      {
        "authentication": "jwt",
        "event": "room_message",
        "pattern": "^\\/([Aa][Ss][Kk][Gg][Rr][Ee][Gg])",
        "url": "https://askgreg.herokuapp.com/askgreg"
      }
    ]
  },
  "description": "Let Greg tell you a pun",
  "key": "com.atlassian.hipchat.askgreg",
  "name": "Ask Greg",
  "vendor": {
    "name": "Terry Moore II", 
    "url": "http://www.terrymooreii.com"
  }
}
